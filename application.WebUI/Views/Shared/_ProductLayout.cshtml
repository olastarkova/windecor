
@{
    Layout = null;
}


<html>
<head>
    @{ Html.RenderPartial("Head"); }
    <meta name="viewport" content="width=device-width" />
    <title>Рольшторы</title>
</head>
<body>
    @{ Html.RenderPartial("Header");
        Html.RenderPartial("TopMenu");

    }
    <section class="rolCollections">
        <div class="container">
            <div class="row">
                <div class="hidden-xs col-sm-3 sideBar">
                    <div class="panel panel-default">
                        <div class="panel-heading text-uppercase">Категории</div>
                        <div class="panel-body">
                            <div id="categories">
                                <ul class="firstLevel rolCategoriesFL">
                                    <li>
                                        <ul class="secondLevel rolCategoriesSL">
                                            @{ Html.RenderAction("Menu", "Nav"); }
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-xs-12  col-sm-9  productContent" id="products">@RenderBody()</div>
            </div>
        </div>
    </section>
    @{ Html.RenderPartial("Footer"); }
</body>



</html>
