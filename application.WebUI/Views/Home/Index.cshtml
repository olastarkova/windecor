
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    @{ Html.RenderPartial("Head"); }
    <meta name="viewport" content="width=device-width" />
    <title>Главная</title>
</head>
<body>
    @{ Html.RenderPartial("Header");
        Html.RenderPartial("TopMenu");

    }
    <section class="slider">
        <div class="container-fluid">
            <div class="row">
                <div id="carousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#carousel" data-slide-to="0" class="active"></li>
                        <li data-target="#carousel" data-slide-to="1"></li>
                        <li data-target="#carousel" data-slide-to="2"></li>
                        <li data-target="#carousel" data-slide-to="3"></li>
                        <li data-target="#carousel" data-slide-to="4"></li>
                        <li data-target="#carousel" data-slide-to="5"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner" role="listbox">
                        <div class="item active">
                            <div class="slide1">
                                <img src="~/Content/img/slide1.jpg" alt="Карнизы Оникс" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-md-4 col-md-offset-7  col-sm-6 col-sm-offset-6 slider_info">
                                        <div>Оникс &Oslash;25мм</div>
                                        <div></div>
                                        <a class="btn text-uppercase" role="button" href="#">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="slide1">
                                <img src="~/Content/img/slide2.jpg" alt="Рольшторы День-Ночь" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-xs-6 col-md-4 col-md-offset-1  col-sm-6 col-sm-offset-1 slider_info">
                                        <div>Багама-2 День-Ночь</div>
                                        <div></div>
                                        <a class="btn text-uppercase" role="button" href="~/Product/Details/18">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="slide1">
                                <img src="~/Content/img/slide3.jpg" alt="Карнизы Square Line" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-md-4 col-md-offset-7  col-sm-6 col-sm-offset-6 slider_info">
                                        <div>Square Line</div>
                                        <div>от 40р.</div>
                                        <a class="btn text-uppercase" role="button" href="#">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="slide1">
                                <img src="~/Content/img/slide4.jpg" alt="Рольшторы Blackout" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-md-4 col-md-offset-1  col-sm-6 col-sm-offset-1 slider_info slider_info_left">
                                        <div>Blackout</div>
                                        <div>от 40р.</div>
                                        <a class="btn text-uppercase" role="button" href="#">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="slide1">
                                <img src="~/Content/img/slide5.jpg" alt="Карнизы Квадро" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-md-4 col-md-offset-7  col-sm-6 col-sm-offset-6 slider_info">
                                        <div>Kwadro</div>
                                        <div>от 40р.</div>
                                        <a class="btn text-uppercase" role="button" href="#">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="slide1">
                                <img src="~/Content/img/slide6.jpg" alt="Карнизы круглые" />
                                <div class="row">
                                    <div class="hidden-xs col-sm-6 col-md-4 col-md-offset-7  col-sm-6 col-sm-offset-6 slider_info">
                                        <div>Kwadro</div>
                                        <div>от 40р.</div>
                                        <a class="btn text-uppercase" role="button" href="#">Подробнее</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Controls -->
                    <a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#carousel" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <section class="catalogue">
        <div class="container">
            <div class="row">
                <header class="block_header">
                    <h3 class="text-uppercase">Популярное:</h3>
                </header>
                <div class="images">
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <div class="image_wraper">
                            <img src="~/Content/img/karnizy-1.jpg" alt="Карнизы" />
                            <div class="masking">
                                <a class="btn text-uppercase" role="button" href="#">В каталог</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <div class="image_wraper">
                            <img src="~/Content/img/karnizy-1.jpg" alt="Карнизы" />
                            <div class="masking">
                                <a class="btn link text-uppercase" role="button" href="#">В каталог</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-4">
                        <div class="image_wraper">
                            <img src="~/Content/img/karnizy-2.jpg" alt="Карнизы" />
                            <div class="masking">
                                <a class="btn link text-uppercase" role="button" href="#">В каталог</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="map">
        <div class="container">
            <div class="row">
                <header class="block_header">
                    <h3 class="text-uppercase">Как нас найти:</h3>
                </header>
            </div>
            <div class="row address">
                <div class="col-xs-12 col-md-3 col-md-offset-3">
                    <span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
                    Стройрынок Уручье
                </div>
                <div class="col-xs-12 col-md-4">ул.Уручская, 13, пом.109А</div>
            </div>
            <div class="row">
                <h4>
                    <span class="glyphicon glyphicon-time" aria-hidden="true"></span>
                    Вт-Вс: 10:00 - 16:30
                </h4>
            </div>
        </div>
        <script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?sid=n_vb0iZBT9C41zbn0c6ME-9CJUQWWgf4&amp;width=100%&amp;height=530&amp;lang=ru_RU&amp;sourceType=constructor&amp;scroll=true"></script>

    </section>
    @{ Html.RenderPartial("Footer"); }
</body>



</html>
